<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.aiyaapp.aiyamediaeditor_android">

    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.READ_PHONE_STATE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.RECORD_AUDIO" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="com.qti.permission.PROFILER" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.aiyaapp.aiyamediaeditor.ui.RecordSettingActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.aiyaapp.aiyamediaeditor.ui.RecordingActivity" />
        <activity android:name="com.aiyaapp.aiyamediaeditor.ui.VedioEditActivity" />
        <activity android:name="com.aiyaapp.aiyamediaeditor.ui.EffectEditActivity" />
        <activity android:name="com.aiyaapp.aiyamediaeditor.ui.SelectMusioActivity" />
        <activity android:name="com.aiyaapp.aiyamediaeditor.ui.VideoSurfaceActivity"></activity>
    </application>

</manifest>